{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noImplicitReturns": false,
    "experimentalDecorators": true,
    "watch": false,
    "outDir": "bin",
    "lib": [
        "es5",
        "es2015"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}